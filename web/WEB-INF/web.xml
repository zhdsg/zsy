<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
    <servlet-name>EbIndexController</servlet-name>
    <servlet-class>sdkd.com.ec.controller.EbIndexController</servlet-class>
</servlet>
    <servlet-mapping>
        <servlet-name>EbIndexController</servlet-name>
        <url-pattern>/index.servlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>RegisterController</servlet-name>
        <servlet-class>sdkd.com.ec.controller.RegisterController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterController</servlet-name>
        <url-pattern>/register.servlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>sdkd.com.ec.controller.LoginController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/login.servlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>EbNewsContController</servlet-name>
        <servlet-class>sdkd.com.ec.controller.EbNewsContController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EbNewsContController</servlet-name>
        <url-pattern>/ncontent.servlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>EbAContentController</servlet-name>
        <servlet-class>sdkd.com.ec.controller.EbAContentController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EbAContentController</servlet-name>
        <url-pattern>/acontent.servlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>EbPContentController</servlet-name>
        <servlet-class>sdkd.com.ec.controller.EbPContentController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EbPContentController</servlet-name>
        <url-pattern>/pcontent.servlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>EbShoppingController</servlet-name>
        <servlet-class>sdkd.com.ec.controller.EbShoppingController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EbShoppingController</servlet-name>
        <url-pattern>/shopping.servlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>MNewsController</servlet-name>
        <servlet-class>sdkd.com.ec.controller.backend.MNewsController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MNewsController</servlet-name>
        <url-pattern>/mnews.servlet</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>/manage/index.jsp</welcome-file>
    </welcome-file-list>

</web-app>